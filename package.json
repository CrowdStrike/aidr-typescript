{
  "name": "@crowdstrike/aidr",
  "version": "0.0.0",
  "description": "TypeScript SDK for CrowdStrike AIDR",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/crowdstrike/aidr-typescript.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "commonjs",
  "types": "dist/index.d.cts",
  "main": "dist/index.cjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./*.mjs": {
      "default": "./dist/*.mjs"
    },
    "./*.cjs": {
      "default": "./dist/*.cjs"
    },
    "./*": {
      "import": "./dist/*.mjs",
      "require": "./dist/*.cjs"
    }
  },
  "scripts": {
    "test": "vitest run",
    "build": "tsdown && tsc --noEmit",
    "prepare": "pnpm build",
    "lint": "biome check"
  },
  "dependencies": {
    "valibot": "^1.1.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.5",
    "@containerbase/semantic-release-pnpm": "1.3.5",
    "@hey-api/openapi-ts": "0.88.0",
    "@stoplight/prism-cli": "5.14.2",
    "@types/node": "24.10.1",
    "conventional-changelog-conventionalcommits": "9.1.0",
    "semantic-release": "25.0.2",
    "start-server-and-test": "2.1.3",
    "tsdown": "0.16.7",
    "typescript": "5.9.3",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "4.0.13"
  }
}
